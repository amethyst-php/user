    "license": "MIT",
    "require": {
        "php": ">=7.0.0",
        "phpstan/phpstan": "^0.9.2",
        "laravel/passport": "^5.0",
        "railken/bag": "^1.2",
        "railken/laravel-manager": "^3.1",
        "zizaco/entrust": "^1.9",
        "laravolt/avatar": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench": "~3.6",
        "friendsofphp/php-cs-fixer": "^2.8.1",
        "spatie/pdf-to-text": "^1.1",
        "railken/lara-command-test": "^1.0"

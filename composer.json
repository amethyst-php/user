{
    "name": "amethyst/user",
    "keywords": ["laravel", "amethyst", "amethyst-user", "user"],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "amethyst/core": "0.2.*",
        "laravolt/avatar": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "orchestra/testbench": "*"
    },
    "autoload": {
        "psr-4" : {
            "Amethyst\\" : "src/",
            "Amethyst\\Database\\Seeds\\": "database/seeds/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amethyst\\Providers\\UserServiceProvider"
            ]
        }
    }
}
